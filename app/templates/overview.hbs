{{page-title "Overview"}}
<AuApp>
  <Header
    @username={{this.username}}
    @isAuthenticated={{this.isAuthenticated}}
  />
  <AuBodyContainer @scroll={{this.scroll}} class="au-c-body-container--scroll">
    <Search @username={{this.username}} />
    <AuHr class="au-u-margin-none" />
    <AppCards
      @apps={{this.application.applications}}
      @isLoading={{this.application.isLoading}}
    />
  </AuBodyContainer>
  <Footer />
</AuApp>